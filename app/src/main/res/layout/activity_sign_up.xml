<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:background="#ffffff"

android:layout_height="match_parent"
tools:context=".SignUp">

<TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="person registration"
    android:textSize="40sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.493"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.023" />

<EditText
    android:id="@+id/pr_email"

    android:layout_height="50dp"
    android:layout_margin="20dp"
    android:layout_width="match_parent"
    android:background="@drawable/shape1"
    android:gravity="center"
    android:ems="10"
    android:hint="Email"
    android:inputType="textEmailAddress"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.497"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView5"
    app:layout_constraintVertical_bias="0.023" />

<EditText
    android:id="@+id/pr_name"
    android:layout_height="50dp"
    android:layout_margin="20dp"
    android:layout_width="match_parent"
    android:background="@drawable/shape1"
    android:gravity="center"
    android:ems="10"
    android:hint="Name"
    android:inputType="textPersonName"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.497"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/pr_email"
    app:layout_constraintVertical_bias="0.055" />

<EditText
    android:id="@+id/pr_password"
    android:layout_height="50dp"
    android:layout_margin="20dp"
    android:layout_width="match_parent"
    android:background="@drawable/shape1"
    android:gravity="center"
    android:ems="10"
    android:hint="password"
    android:inputType="textPassword"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.497"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/pr_name"
    app:layout_constraintVertical_bias="0.08" />

<EditText
    android:id="@+id/editText"
    android:layout_height="50dp"
    android:layout_margin="20dp"
    android:layout_width="match_parent"
    android:background="@drawable/shape1"
    android:gravity="center"
    android:ems="10"
    android:hint="phone"
    android:inputType="phone"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.497"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/pr_password"
    app:layout_constraintVertical_bias="0.057" />

<Spinner
    android:id="@+id/personal_registration_spinner"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:layout_margin="20dp"

    android:background="@drawable/spinner_design"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.6"

    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/editText"
    app:layout_constraintVertical_bias="0.068" />

<TextView
    android:id="@+id/textView2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="choose blood type"
    app:layout_constraintBottom_toBottomOf="@+id/personal_registration_spinner"
    app:layout_constraintEnd_toEndOf="@+id/personal_registration_spinner"
    app:layout_constraintHorizontal_bias="0.144"
    app:layout_constraintStart_toStartOf="@+id/personal_registration_spinner"
    app:layout_constraintTop_toTopOf="@+id/personal_registration_spinner"
    app:layout_constraintVertical_bias="0.476" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/ttttt"
    android:text="choose city"
    app:layout_constraintBottom_toBottomOf="@+id/city_spinner"
    app:layout_constraintEnd_toEndOf="@+id/city_spinner"
    app:layout_constraintHorizontal_bias="0.368"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/city_spinner"
    app:layout_constraintVertical_bias="0.571" />

<Spinner
    android:id="@+id/city_spinner"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:layout_margin="20dp"
    android:background="@drawable/spinner_design"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.925"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/personal_registration_spinner"
    app:layout_constraintVertical_bias="0.047" />

<androidx.appcompat.widget.AppCompatButton
    android:id="@+id/bt_reg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="register"
    android:textAllCaps="false"
    android:background="@drawable/custom_botton"
    android:textSize="20sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/city_spinner"
    app:layout_constraintVertical_bias="0.118" />

<TextView
    android:id="@+id/logIn_text"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="click here if you have account!"
    android:textSize="20sp"

    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/bt_reg"
    app:layout_constraintVertical_bias="0.161" />
</androidx.constraintlayout.widget.ConstraintLayout>